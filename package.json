{
  "name": "halla",
  "version": "0.0.1",
  "main": "index.html",
  "devDependencies": {
    "nwjs-builder": "1.13.1",
    "rc": "1.1.6",
    "tape": "4.6.0"
  },
  "scripts": {
    "start": "nwb nwbuild -v ${npm_package_config_nwjs_version} ${npm_package_config_nwb_opts} -r .",
    "start:sdk": "nwb nwbuild -v ${npm_package_config_nwjs_version}-sdk ${npm_package_config_nwb_opts} -r .",
    "dist": "nwb nwbuild -v $npm_package_config_nwjs_version ${npm_package_config_nwb_opts} -p osx64 -o dist && ./support/post-dist"
  },
  "dependencies": {
    "101": "1.6.1",
    "classnames": "2.2.5",
    "decca": "2.1.0",
    "lodash": "4.13.1",
    "nwjs-builder": "1.13.1",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  },
  "chromium-args": "--mixed-context",
  "config": {
    "nwjs_version": "0.16.0",
    "nwb_opts": "--mac-icns assets/halla.icns"
  }
}
